    "browser-test": "zuul --server test/browser/server.js --local --open test/browser/test.js",
    "sauce-test": "zuul --server test/browser/server.js --tunnel ngrok -- test/browser/test.js",
    "ci": "npm run tslint && npm run test && codecov"
  },
